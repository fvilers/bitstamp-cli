{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "windows"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": "off"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended"
}
